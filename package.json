{
  "main": "node_modules/expo/AppEntry.js",
  "name": "come-buy",
  "version": "1.0.0",
  "scripts": {
    "start": "APP_ENV=development npx expo start",
    "start:clear": "npx expo start --clear",
    "start:client": "npx expo start --dev-client",
    "start:client:clear": "npx expo start --dev-client --clear",
    "prebuild:android:clean": "expo prebuild --platform android --clean",
    "prebuild:android": "expo prebuild --platform android",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "eject": "npx expo eject",
    "build:android:prod": "APP_ENV=production eas build --platform android --profile production",
    "build:android:dev": "APP_ENV=development eas build --platform android --profile development",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --coverage",
    "test:badges": "yarn test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__",
    "lint": "npx eslint --max-warnings=0 --ext .tsx,.ts,.js,.jsx src/ --fix",
    "eas:update:android:dev": "APP_ENV=development eas update --auto -p android",
    "eas:update:android:prod": "APP_ENV=production eas update --auto -p android",
    "eas:submit:android": "eas submit --platform android --latest"
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "0.2.8",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/material-bottom-tabs": "^6.2.4",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/stack": "^6.3.2",
    "@reduxjs/toolkit": "^1.6.1",
    "expo": "^47.0.0",
    "expo-build-properties": "~0.4.1",
    "expo-constants": "~14.0.2",
    "expo-dev-client": "~2.0.1",
    "expo-font": "~11.0.1",
    "expo-linking": "~3.3.0",
    "expo-location": "~15.0.1",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.15.6",
    "formik": "^2.2.9",
    "geodist": "^0.2.1",
    "lottie-react-native": "5.1.4",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-flash-message": "^0.3.1",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-pager-view": "6.0.1",
    "react-native-paper": "^4.12.4",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-svg": "13.4.0",
    "react-native-tab-view": "^3.2.1",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.18.7",
    "react-navigation": "^4.4.4",
    "react-navigation-stack": "^2.10.4",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-unicode-property-regex": "^7.18.6",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^5.0.0",
    "@testing-library/react-native": "^11.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.1.2",
    "@types/react-native": "~0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "deprecated-react-native-prop-types": "^2.3.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "jest-expo": "^47.0.0",
    "metro-babel-register": "^0.73.1",
    "metro-react-native-babel-preset": "0.72.3",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3"
  },
  "private": true,
  "description": "A simple social e-commerce app",
  "keywords": [
    "expo",
    "typescript",
    "react-native",
    "fake store api",
    "jest",
    "come-buy",
    "mobile",
    "Etap",
    "Assessment test"
  ],
  "homepage": "https://github.com/AdamuAbba/come_buy.git#readme",
